@model IEnumerable<Red_Lake_Hospital_Redesign_Team6.Models.FaqDto>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Question #
        </th>
        <th>
            Department
        </th>
        <th>
            Question
        </th>
        <th>
            Answer
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.faq_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.answer)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.faq_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.faq_id }) |
                @Html.ActionLink("Delete", "DeleteConfirm", new { id = item.faq_id })
            </td>
        </tr>
    }
    </table>
